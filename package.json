{
  "name": "@aybuben/rc",
  "version": "1.0.5",
  "description": "",
  "private": true,
  "scripts": {
    "test": "lerna run test",
    "wizard": "npx lerna-wizard",
    "diff": "lerna diff",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:publish": "lerna publish --conventional-commits",
    "lerna:commit": "lerna version --conventional-commits --yes",
    "create:package": "lerna create $npm_config_package_name",
    "lerna:exec": "lerna exec -- $npm_config_command"
  },
  "author": "har-sargis",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/har-sargis/publish_test.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.3.8",
    "lerna": "^4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "workspaces": [
    "packages/*"
  ]
}
